<%= content_for :menu do %>
<% end %>

<div class="login_title">
  <h1>Log in</h1>
</div>
<%= form_tag sessions_path do %>
  <div class="field">
    <%= label_tag :email %><br />
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div class="field">
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
  </div>
  <p class="button"><%= submit_tag "Log in" %></p>
<% end %>

